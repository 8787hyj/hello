package com.epbooks.www.search.dto;

//커맨드 객체
public class User {

	String inputName_1;
	String inputPhone_1;
	String inputId_2;
	String inputEmail_2;
	
	public User() {
		// TODO Auto-generated constructor stub
	}
	
	public User(String inputName_1, String inputEmail_2) {
		this.inputName_1 = inputName_1;
		this.inputEmail_2 = inputEmail_2;
		
	}
	
	public User(String inputName_1, String inputId_2, String inputEmail_2) {
		this.inputName_1 = inputName_1;
		this.inputId_2 = inputId_2;
		this.inputEmail_2 = inputEmail_2;
	}
	
	public User(String inputName_1, String inputPhone_1, String inputId_2, String inputEmail_2) {
		this.inputName_1 = inputName_1;
		this.inputPhone_1 = inputPhone_1;
		this.inputId_2 = inputId_2;
		this.inputEmail_2 = inputEmail_2;
	}
	public String getInputName_1() {
		return inputName_1;
	}
	public String getInputPhone_1() {
		return inputPhone_1;
	}
	public String getInputId_2() {
		return inputId_2;
	}
	public String getInputEmail_2() {
		return inputEmail_2;
	}

	@Override
	public String toString() {
		return "User [inputName_1=" + inputName_1 + ", inputPhone_1=" + inputPhone_1 + ", inputId_2=" + inputId_2
				+ ", inputEmail_2=" + inputEmail_2 + "]";
	}
	
	
}
